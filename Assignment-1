--products list
select ProductName from Products
--products names and prices
select ProductName, UnitPrice  from Products
--products that cost more then 50
select ProductName, UnitPrice,CategoryID from Products where UnitPrice >50  
--products their prices are between 40-50
select ProductName, UnitPrice,CategoryID from Products where UnitPrice between 40 and 50 
--The product names, prices and category number for the products that cost more than 50 dollars for category 1 or category 2
select ProductName, UnitPrice,CategoryID from Products where (CategoryID=1  or  CategoryID=2) and UnitPrice >50  
--name Supplier
select SupplierID from Products 
